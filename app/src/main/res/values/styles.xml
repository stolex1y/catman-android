    <style name="BodyText" parent="Widget.AppCompat.TextView">
        <item name="android:textAppearance">?attr/textAppearanceBody2</item>
        <item name="textColor">?android:attr/textColor</item>
        <item name="lineHeight">17sp</item>
    </style>


    <style name="OutlinedButton" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="textAllCaps">false</item>
        <item name="android:paddingStart">16dp</item>

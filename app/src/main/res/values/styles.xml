        <item name="textAllCaps">false</item>
        <item name="shapeAppearance">?attr/shapeAppearanceSmallComponent</item>
    </style>

    <style name="NavigationView" parent="Widget.MaterialComponents.NavigationView">
        <item name="itemTextAppearance">?attr/textAppearanceHeadline6</item>
        <item name="itemTextColor">?android:attr/textColor</item>
        <item name="itemShapeFillColor">@color/transparent</item>

        <item name="shapeAppearance">?attr/shapeAppearanceLargeComponent</item>
        <item name="backgroundTint">?attr/backgroundColor</item>
        <item name="shapeAppearanceOverlay">?attr/shapeAppearanceLargeComponent</item>
    </style>

    <style name="ModalBottomSheetDialog" parent="ThemeOverlay.MaterialComponents.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/ModalBottomSheet</item>
        <item name="paddingBottomSystemWindowInsets">false</item>
        <item name="paddingTopSystemWindowInsets">false</item>
        <item name="paddingLeftSystemWindowInsets">false</item>
        <item name="paddingRightSystemWindowInsets">false</item>
    </style>

    <style name="TextInputLayout.ExposedDropdownMenu" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="materialThemeOverlay">@style/Overlay</item>

        <item name="errorIconTint">?attr/colorError</item>
        <item name="startIconTint">?android:attr/textColor</item>
        <item name="endIconTint">?android:attr/textColor</item>

        <item name="boxStrokeErrorColor">?attr/colorError</item>
        <item name="boxStrokeColor">@drawable/box_stroke_color</item>
        <item name="boxStrokeWidth">2dp</item>
        <item name="boxStrokeWidthFocused">2.5dp</item>

        <item name="errorTextAppearance">?attr/textAppearanceOverline</item>
        <item name="hintTextAppearance">?attr/textAppearanceOverline</item>

        <item name="hintTextColor">?android:attr/textColor</item>
        <item name="android:textColorHint">?attr/hintTextColor</item>
        <item name="errorTextColor">?attr/errorTextColor</item>

        <item name="hintEnabled">true</item>

        <item name="shapeAppearance">?attr/shapeAppearanceSmallComponent</item>
        <item name="shapeAppearanceOverlay">?attr/shapeAppearanceSmallComponent</item>
    </style>

    <style name="TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="materialThemeOverlay">@style/Overlay</item>
        <item name="shapeAppearance">?attr/shapeAppearanceSmallComponent</item>

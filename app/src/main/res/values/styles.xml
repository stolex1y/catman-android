        <item name="errorTextAppearance">?attr/textAppearanceOverline</item>
        <item name="android:textAppearance">?attr/textAppearanceBody2</item>
        <item name="hintTextAppearance">?attr/textAppearanceBody2</item>
        <item name="textAppearanceOverline">?attr/textAppearanceOverline</item>

        <item name="android:textColor">?android:attr/textColor</item>
        <item name="hintTextColor">?attr/hintTextColor</item>
        <item name="android:textColorHint">?attr/hintTextColor</item>
        <item name="errorTextColor">?attr/errorTextColor</item>

        <item name="errorEnabled">true</item>
        <item name="hintEnabled">true</item>
    </style>

    <style name="EditText" parent="Widget.Material3.TextInputEditText.OutlinedBox">
        <item name="android:textAppearance">?attr/textAppearanceBody2</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="lineHeight">17sp</item>

        <item name="strokeWidth">2dp</item>
        <item name="backgroundTint">@color/transparent</item>
        <item name="android:outlineProvider">background</item>
    </style>

    <style name="ContainedButton" parent="Widget.MaterialComponents.Button">
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
        <item name="backgroundTint">@drawable/button_background</item>
        <item name="android:textColor">?android:attr/textColor</item>

    <attr name="ay_offStateStyle" format="reference" />
    <attr name="ay_progressShadowColor" format="color" />
    <attr name="ay_progressColor" format="color" />
    <attr name="ay_progressStartColor" format="color" />
    <attr name="ay_progressMidColor" format="color" />
    <attr name="ay_progressEndColor" format="color" />
    <attr name="ay_progressShadowRadius" format="dimension" />
